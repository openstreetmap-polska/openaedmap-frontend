    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "lefthook": "^1.6.1",
    "sass": "^1.70.0",
    "vite": "^5.1.1",
    "vite-plugin-sitemap": "^0.5.3",
    "vitest": "^1.2.2"
  }
